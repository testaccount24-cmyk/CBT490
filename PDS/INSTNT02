//TSO057X JOB (T951V57),                                                JOB00892
//           CLASS=A,MSGCLASS=T,
//           NOTIFY=TSO057
//*
//        INCLUDE MEMBER=SYSLEV
//*
//*
//*        ASSEMBLE, LINK AND RUN THE IVP PROGRAM
//*
//*        THIS PROGRAM USES INSTGET TO GET THE NAME/TOKEN STRING
//*        AND THEN DISPLAYS IT
//*
//********************************************************************
//*
//* AFTER YOU HAVE DONE THIS, YOU MAY ELABORATE AS YOU PLEASE.
//*
//* INSTGET CAN BE COMPILED TO OBJECT CODE AND SO MADE CALLABLE FROM
//* ANY PROGRAM THAT CAN HANDLE OS LINKAGE (COBOL, C, PLI).
//*
//*
//*
//*
//         EXEC PGM=INSTSET,PARM='KEY1=FRED,KEY2=JONES'
//*
//*
//ASM      EXEC PGM=ASMA90,
// PARM='NORLD,XREF(SHORT),DECK,NOOBJECT,LIST,TERM,RENT'
//SYSTERM  DD   SYSOUT=*
//SYSPRINT DD   SYSOUT=*
//SYSUT1   DD   UNIT=SYSALLDA,SPACE=(1700,(600,600))
//SYSLIN   DD   DUMMY
//SYSPUNCH DD   DSN=&&OBJ,DISP=(NEW,PASS),
//         SPACE=(80,(1500,750)),UNIT=SYSALLDA,BLKSIZE=3120
//SYSLIB   DD   DSN=SFT.INSTASM.ASM,DISP=SHR
//         DD   DSN=SYS1.MACLIB,DISP=SHR
INSTNT02 INSTSS

         XC      STRING,STRING
         CALL    INSTGET,(STRING),VL,MF=(E,PL)
         CALL    INSTWTOR,(STRING,=A(L'STRING)),VL,MF=(E,PL)

         INSTRS  RC=0



         INSTWORK START

PL        CALL  ,(,,,,),MF=L

STRING    DS       CL200' '
          INSTWORK END






          END
          COPY INSTGET
          COPY INSTWTOR
//*
//*
//LINK     EXEC PGM=HEWL,COND=(0,LT),
// PARM=('XREF,LIST,MAP',
// 'AMODE=31,RMODE=ANY,RENT,REUS')
//SYSUT1   DD   UNIT=SYSALLDA,SPACE=(1024,(100,30))
//SYSPRINT DD   SYSOUT=*
//SYSLIN   DD   DSN=&&OBJ,DISP=(OLD,DELETE)
//SYSLIB   DD   DISP=SHR,DSN=SYS1.&SYSLEV..CSSLIB
//SYSLMOD  DD DSN=&&LOAD(INSTNT02),DISP=(,PASS),SPACE=(99,(99,999,1)),
//         UNIT=SYSALLDA
//*
//RUN      EXEC PGM=INSTNT02
//STEPLIB  DD   DSN=&&LOAD,DISP=(OLD,DELETE)
